<script>

/* function for submit contact */
 function submitForm() {
   event.preventDefault();
   var $form = $( this ),
       url = "/sendEmail/";
   var posting = $.post( url, { nameId: $('#nameId').val(), emailId: $('#emailId').val(), requestTypeId: $('#requestTypeId').val(), descriptionId: $('#descriptionId').val(), checkboxId: $('#checkboxId').val() } );

   posting.done(function( data ) {
     if (data == "success")
       document.getElementById('overlay-custom-close-demo-success-email-open').click();
     else
       document.getElementById('overlay-custom-close-demo-error-email-open').click();
   });
 }

 /* function for play video */
  var tag = document.createElement('script');

  tag.src = "https://www.youtube.com/iframe_api";
  var firstScriptTag = document.getElementsByTagName('script')[0];
  firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

  var player;
  function onYouTubeIframeAPIReady() {
    player = new YT.Player('player', {
      height: '390',
      width: '640',
      videoId: 'm9jT6nStyCQ',
      events: {
        'onStateChange': onPlayerStateChange
      }
    });
  }

  function onPlayerReady(event) {
    event.target.playVideo();
  }

  var done = false;
  function onPlayerStateChange(event) {
    if (event.data == YT.PlayerState.PLAYING && !done) {
      //setTimeout(stopVideo, 60000);
      done = true;
    }
  }
  function stopVideo() {
     $("#ytplayer").stopVideo();
}



  document.getElementById("overlay-custom-close-demo-success-email-open").style.display = "none";
  document.getElementById("overlay-custom-close-demo-error-email-open").style.display = "none";

  $(function(){ //south_america_mill
    $('#map').vectorMap({
      markers: [
        {id: 1, latLng: [-22.950421, -43.176945], name: 'Rio de Janeiro - Domestic and Exportation Delivery'},
        {id: 2, latLng: [-19.975513, -43.946277], name: 'Belo Horizonte - Domestic and Exportation Delivery'},
        {id: 3, latLng: [-30.046148, -51.229865], name: 'Rio Grande do Sul - Domestic Delivery'},
        {id: 4, latLng: [-23.579866, -46.648860], name: 'São Paulo - Main Domestic Delivery'},
        {id: 5, latLng: [-22.899421, -47.200635], name: 'Hortolândia - Main Domestic and Exportation Delivery'}
      ],
      map: 'south_america_mill',
      scaleColors: ['#367AA3', '#0071A4'],
      normalizeFunction: 'polynomial',
      hoverOpacity: 10.7,
      hoverColor: true,
      markerStyle: {
        initial: {
          fill: '#27AA7C',
          stroke: '#367AA3'
        }
      },
      backgroundColor: '#A9AFB4',
  });
});
</script>

